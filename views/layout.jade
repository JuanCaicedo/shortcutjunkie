			div.container
				div.navbar-header
					button.navbar-toggle(type="button" data-toggle="collapse" data-target="#navbarCollapse")
						span(class="sr-only") Toggle Navigation
						span(class="icon-bar")
						span(class="icon-bar")
						span(class="icon-bar")
					a.navbar-brand(href="#") #{title}
				div.collapse.navbar-collapse#navbarCollapse
					ul.nav.navbar-nav(role="menu")
						li
							a(data-toggle="collapse" data-target="#navbarCollapse" ng-href = "/#/partials/shortcuts") Shortcuts Home
						li
							a(data-toggle="collapse" data-target="#navbarCollapse" ng-href = "/#/partials/search") Search
				
